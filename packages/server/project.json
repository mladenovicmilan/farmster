{
  "name": "server",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "targets": {
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "yarn dev",
        "cwd": "packages/server"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "yarn build",
        "cwd": "packages/server"
      },
      "outputs": ["{projectRoot}/dist"]
    },
    "start": {
      "executor": "nx:run-commands",
      "options": {
        "command": "yarn serve",
        "cwd": "packages/server"
      }
    }
  }
}